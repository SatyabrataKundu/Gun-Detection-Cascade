<div class="llogin">
    <a id="n_contact"></a>
    <span class="idi_separador">|</span>
    <button id="idbtn-link" class="btn-link dropdown-toggle" type="button" data-toggle="dropdown">
        <span id="txt_login" /><span class="caret"></span>
    </button>
    <a id="txt_logout" style="display: none;"></a>
    <span class="idi_separador">|</span>
    <samp id="bienvenidos_PeruInfo"></samp>
</div>

<script type="text/javascript">

    var url = location.href.split('/');

    $('#txt_login').on('click', function () {
        var page = 'http://' + url[2] + '/' + url[3] + "/login";
        window.location.href = page;
    });

    switch (url[3]) {
        case 'es-pe':
            $('#txt_login').text('Inicia sesión');
            $('#txt_logout').text('Salir');
            $('#n_contact').text('Contáctanos').attr('href', '/es-pe/contactanos');
            $('#iPopUp').attr('src', '/es-pe/Login');
            $('.Idioma .btn-link').html('<span>Español</span> <span class="caret"></span>');
            break;

        case 'en-us':
            $('#txt_login').text('Login');
            $('#txt_logout').text('Logout');
            $('#n_contact').text('Contact us').attr('href', '/en-us/contact-us');
            $('#iPopUp').attr('src', '/en-us/Login');
            $('.Idioma .btn-link').html('<span>English</span> <span class="caret"></span>');
            $('.Idioma .dropdown-menu li:eq(0)').html('<a href="/es-pe/">Peru</a>');
            $('.Idioma .dropdown-menu li:eq(1)').html('<a href="/es-lat/">Latin America</a>');
            break;

        <!-- case 'es-lat': -->
            <!-- $('#txt_login').text('Inicia sesión'); -->
            <!-- $('#txt_logout').text('Salir'); -->
            <!-- $('#n_contact').text('Contáctanos').attr('href', '/es-lat/contactanos'); -->
            <!-- $('#iPopUp').attr('src', '/es-lat/Login'); -->
            <!-- $('.Idioma .btn-link').html('<span>Latinoámerica</span> <span class="caret"></span>'); -->
            <!-- $('.Idioma .dropdown-menu li:eq(0)').html('<a href="/es-pe/">Perú</a>'); -->
            <!-- $('.Idioma .dropdown-menu li:eq(1)').html('<a href="/en-us/">Inglés</a>'); -->
            <!-- break; -->
    }

    $(document).ready(function () {

        $("a#txt_logout").click(function () { function1().done(function () { function2().done(function () { }); }); });

        var moduleId = parseInt("[ModuleContext:ModuleId]");

        var service = {
            path: "BBSPA",
            framework: $.ServicesFramework(moduleId)
        }

        service.baseUrl = service.framework.getServiceRoot(service.path) + "PI_REGISTRO/";

        $('#bienvenidos_PeruInfo').hide();

        var getLista = function () {
            return $.ajax({
                url: service.baseUrl + "GetLista",
                beforeSend: service.framework.setModuleHeaders,
                dataType: "json"
            }).done(getSucceeded).fail(getFailed);

            function getSucceeded(data) {
                var perfil_nombre = data.nombre, perfil_apellido = data.apellido, bienvenido = '', portal = window.location.href;
                portal = portal.split('/');
                var resultadoPortal = portal[3], USERID = '-1';

                if (USERID > 0) {
                    $('#bienvenidos_PeruInfo').show();
                } else {
                    $('#bienvenidos_PeruInfo').hide();
                }

                if (resultadoPortal == 'es-pe') {
                    $('#bienvenidos_PeruInfo').html("Hola: " + perfil_nombre + ", " + perfil_apellido);
                } else if (resultadoPortal == 'es-lat') {
                    $('#bienvenidos_PeruInfo').html("Hola: " + perfil_nombre + ", " + perfil_apellido);
                } else if (resultadoPortal == 'en-us') {
                    $('#bienvenidos_PeruInfo').html("Hello: " + perfil_nombre + ", " + perfil_apellido);
                }
            }
            function getFailed() { errorObservable("no data"); }
        };

        getLista();

    });

    var function1 = function () {
        var dfrd1 = $.Deferred();
        setTimeout(function () {
            IN.User.logout();
            dfrd1.resolve();
        }, 1500);
        return dfrd1.promise();
    }

    var function2 = function () {

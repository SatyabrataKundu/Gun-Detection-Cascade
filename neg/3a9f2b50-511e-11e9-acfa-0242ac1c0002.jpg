	var month = date.getUTCMonth()+1;
	var today = date.getUTCFullYear() + '-' +
		(month<10 ? '0' : '') + month + '-' +
		(day<10 ? '0' : '') + day;
	
	var jqxhr = $.ajax({
		method: "GET",

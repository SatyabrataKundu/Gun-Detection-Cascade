        <script type="text/template" id="ss-renderer-facebook">
    <div class="tile social facebook"><div>
        <div class="content-box">
            <% if (hasOwnProperty("from")) { %>
                <a href="http://www.facebook.com/<%- from.id %>" class="user">
                    <strong><span class="name"><%= from.name %></span></strong>
                </a>
            <% } %>
            <div class="info">
                <% if (hasOwnProperty("message")) { %>
                    <% var msg = message.substr(0, 160); %>
                    <% if (message.length > 160) { msg += " [...]"; } %>
                    <div class="text"><%= msg %></div>
                <% } %>
                <% if (hasOwnProperty("attachments")) { %>
                    <div class="post-attachment">
                        <a href="<%- attachments.data[0].url %>" target="_blank"  class="title-link">
    						<% if (attachments.data[0].media !== undefined) { %>
                                <span class="thumb" data-src="<%- attachments.data[0].media.image.src %>" data-alt=""></span>
                            <% } else { %>
                                <% if (attachments.data[0].subAttachments !== undefined) { %>
    								<% if (attachments.data[0].subAttachments.data[0].media !== undefined) { %>
    									<span class="thumb" data-src="<%- attachments.data[0].subAttachments.data[0].media.image.src %>" data-alt=""></span>
    								<% }; %>
                                <% }; %>
                            <% } %>                                                
                            <% if (attachments.data[0].title) { %>
                                <span class="title"><%= attachments.data[0].title %></span>
                            <% } else if (attachments.data[0].description) { %>
                                <% var desc = attachments.data[0].description.substr(0, 120); %>                                                                
                                <% if (attachments.data[0].description.length > 120) { desc += " [...]"; } %>
                                <span class="description"><%= desc %></span>                                                    
                            <% } else { %>
                                <span class="offscreen">No description available</span>
                            <% } %>  
                        </a>
                    </div>
                <% } %>
                <span class="date"><%= date %></span> 
                <div class="action-buttons">
                    <button value="https://twitter.com/share?text=<%- from.name %>&url=https://www.facebook.com/<%- id %>" class="button action-button gicon-twitter" aria-label="share this post on twitter"><span class="text">Twitter</span></button>
                    <button value="https://plus.google.com/share?url=https://www.facebook.com/<%- id %>" class="button action-button gicon-gplus" aria-label="share this post on google plus"><span class="text">Google Plus</span></button>
                </div>                                                                                                 
            </div>
        </div>                                            
    </div></div>
</script>

<script type="text/template" id="ss-renderer-twitter">
    <div class="tile social twitter"><div>
        <div class="content-box">
            <a href="https://www.twitter.com/<%- user.screen_name %>" class="user" target="_blank">
                <img src="<%- user.profile_image_url_https %>" alt="" class="logo" />
                <strong>
                    <span class="name"><%= user.name %></span>
                    <span class="username"><%= user.screen_name %></span>     
                </strong>
            </a>
            <div class="info">
                <% if (media_entities.length) { %>
					<div class="thumb" data-src="<%- media_entities[0].media_url_https %>" data-alt="<%- media_entities[0].alt_text %>"></div>
                <% } %>                                           
                <span class="tweet"><%= linkedText %></span>
                <span class="date"><%= date %></span> 
                <div class="action-buttons">
                    <button class="button action-button gicon-reply" value="https://twitter.com/intent/tweet?in_reply_to=<%- id_str %>" aria-label="reply this tweet"><span class="text">Reply</span></button>
                    <button class="button action-button gicon-retweet" value="https://twitter.com/intent/retweet?tweet_id=<%- id_str %>" aria-label="retweet this tweet"><span class="text">Retweet</span></button>
                    <button class="button action-button gicon-favorite" value="https://twitter.com/intent/favorite?tweet_id=<%- id_str %>" aria-label="favorite this tweet"><span class="text">Favorite</span></button>
                </div>                
            </div>
        </div>                                        
    </div></div>
</script>

<script type="text/template" id="ss-renderer-instagram">
    <div class="tile social instagram"><div>
        <div class="content-box">
            <a href="https://instagram.com/<%- user.username %>" class="user" target="_blank">
                <img src="<%- user.profile_picture %>" alt="" class="logo" />
                <strong>
                    <span class="name"><%= user.full_name %></span>
                    <span class="username"><%= user.username %></span>
                </strong>
            </a>
            <div class="info">
                <a href="<%- link %>" target="_blank" class="title-link">
                    <span class="thumb" data-src="<%- images.standard_resolution.url %>" data-alt=""></span>
                    <% if (caption != null) { %>
                        <% var captionText = caption.text.substr(0, 160); %>
                        <% if (caption.text.length > 160) { captionText += " [...]"; } %>
                        <span class="title"><%= captionText %></span>
                    <% }; %>
                </a>
                <span class="date"><%= date %></span>
                <div class="action-buttons">
                    <button value="http://www.facebook.com/sharer.php?u=<%- link %>" class="button action-button gicon-facebook" aria-label="share this post on facebook"><span class="text">Facebook</span></button>
                    <button value="https://twitter.com/share?url=<%- link %>" class="button action-button gicon-twitter" aria-label="share this post on twitter"><span class="text">Twitter</span></button>
                    <button value="https://plus.google.com/share?url=<%- link %>" class="button action-button gicon-gplus" aria-label="share this post on google plus"><span class="text">Google Plus</span></button>
                </div>                
            </div>
        </div>
    </div></div>

</script>

<script type="text/template" id="ss-renderer-youtube">
    <div class="tile social youtube"><div>
        <div class="content-box">
            <a href="https://www.youtube.com/channel/<%- snippet.channelId %>" class="user">
                <strong><span class="name"><%= snippet.channelTitle %></span></strong>
            </a>
            <div class="info">
                <% var link = "https://youtube.com/watch?v=" + snippet.resourceId.videoId %>
                <a href="<%- link %>" class="title-link" target="_blank">
                    <% if (snippet.thumbnails.medium) { %>
                        <span class="thumb" data-src="<%- snippet.thumbnails.medium.url %>" data-alt=""></span>
                    <% }; %>
                    <span class="text"><%= snippet.title %></span>
                </a>
                <span class="date"><%= date %></span>
                <div class="action-buttons">
                    <button value="http://www.facebook.com/sharer.php?u=<%- link %>" class="button action-button gicon-facebook" aria-label="share this video on facebook"><span class="text">Facebook</span></button>
                    <button value="https://twitter.com/share?url=<%- link %>" class="button action-button gicon-twitter" aria-label="share this video on twitter"><span class="text">Twitter</span></button>
                    <button value="https://plus.google.com/share?url=<%- link %>" class="button action-button gicon-gplus" aria-label="share this video on google plus"><span class="text">Google Plus</span></button>
                </div>                                                            
            </div>
        </div>                                    
    </div></div>

</script>

<script type="text/template" id="ss-renderer-headline">
    <div class="tile headline <%- type %>"><div>
        <div class="content-box">
            <div class="info">
                <a href="<%- url %>" class="title-link">
                    <span class="thumb" data-src="<%- thumbnailURL %>" data-alt=""></span>
                    <span class="title"><%= title %></span>
                </a>
                <% if (date.length) { %>
                    <span class="date"><%= date %></span>
                <% } %>                
                <% if (sport.length) { %>
                    <span class="sport"><%= sport %></span>  
                <% } %>
                <% if (relatedLinks.length) { %>
                    <div class="related-links">
                    	<ul class="clearfix">
                    	<% var randomChars = ""; %>
                        <% _.each(relatedLinks, function(link,index){ %>
                        	<% randomChars += "|" %>
                            <li><a href="<%- link.url %>" aria-label="<%- link.text %> for <%- title %><%- randomChars %>"><%= link.text %></a></li>
                        <% }); %>
                        </ul>
                    </div>
                <% } %>
            </div>
        </div>                                            
    </div></div>

</script>

<script type="text/template" id="ss-renderer-filter">
    <% var ariaLabel = type + " filter"; %>
    <% if (type === "none") { %>

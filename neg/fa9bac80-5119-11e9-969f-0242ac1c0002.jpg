	<li>
		<a href="#" class="filter <%- type %>-filter" data-type="<%- type %>" role="button" aria-label="<%- ariaLabel %>">
			<span class="icon <%- icon %>"></span><span class="text"><%= text %></span>
		</a>
	</li>
</script>
    

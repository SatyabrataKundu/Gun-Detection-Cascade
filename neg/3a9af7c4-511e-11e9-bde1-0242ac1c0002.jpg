		// Serve the disclosure, and be smarter about branching
		var dp = settings.position.toLowerCase();
		if(dp != 'top' && dp!= 'bottom') {
			dp = 'top';
		}	
		var opts = { in: null, out: null};
		if(dp == 'top') {
			opts.in = {'top':'0'};
			opts.out = {'top':'-300'};
		} else {
			opts.in = {'bottom':'0'};
			opts.out = {'bottom':'-300'};
		}		


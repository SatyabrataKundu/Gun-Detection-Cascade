<script>
if (/.*\.rai\.it/.test(document.location.hostname)) {
  document.domain = 'rai.it';
}
// fix half boxes in .colDx
$('.colDx .box.half').each(function() {
    var t, c;
    t = $(this);
    c = t.prevUntil('.box:not(.half)').length;
    if (c % 2 == 0) {
      t.removeClass('right');
      t.addClass('left');
    } else {
      t.addClass('right');
      t.removeClass('left');
    }
    //console.log(t, c);
  }
);
// device detection
function initTouchMenu() {
  var menu, menuBtn;
  menu = $('.menuTematiche');
  menuBtn = menu.find('span.menuIcon');
  menu.unbind('touchstart touchend click');
  menuBtn.unbind('touchstart touchend click');
  menu.bind('touchend', function() {
    // just need to bind a click handler to make it receive simulated hover events
    var prev, par;
    menu.toggleClass('expanded');
    if (!menu.is('.expanded')) {
      par = menuBtn.parent();
      prev = menuBtn.prev();
      menuBtn.remove();
      window.setTimeout(function() {
        if (prev.length > 0) {
          menuBtn.insertBefore(prev);
        } else {
          par.prepend(menuBtn);
        }
      }, 0);
    }
  });
};
if ('ontouchstart' in window) {
  initTouchMenu();
}


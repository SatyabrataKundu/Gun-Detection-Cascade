function riordinoBoxMobile(){
	if($(window).width()<767){
		if((currPageType == 'tgr') || (currPageType == 'regione')){
			if($('.colCx').length){
				// pagina enhanced 312
				$('#Focus').attr('data-sort', '1').addClass('sort');
				$('.colSx').attr('data-sort', '2').addClass('sort');
				$('.colDx').attr('data-sort', '3').removeClass('colDx').addClass('colSx sort');
			  	$('.colCx').attr('data-sort', '4').removeClass('colCx').addClass('colSx sort');
			  	$('.rubricheTgr').parent().attr('data-sort', '5').addClass('sort');
		  	}else{
			  	// pagina enhanced 222
		  		$('.colDx').removeClass('colDx').addClass('colSx');
		  	}
	
		  	$('.sort').sort(function (a, b) {
				return $(a).data('sort') - $(b).data('sort');
			}).each(function (_, container) {
				$(container).parent().append(container);
			});
	  	}else{
			(function() {
			    var ap, c3a, c2a, c1a, ed, c3c, c2c, c1c, tem;
			
			    ap = $('#Focus');
			    ed = $('#editorChoice').parent();
			    c3a = ed.prevAll('.colSx'); // c3a = c3a.length > 0 ? c3a : false;
			    c1a = ed.prevAll('.colCx'); // c1a = c1a.length > 0 ? c1a : false;
			    c2a = ed.prevAll('.colDx'); // c2a = c2a.length > 0 ? c2a : false;
			    c3c = ed.nextAll('.colSx'); // c3c = c3c.length > 0 ? c3c : false;
			    c1c = ed.nextAll('.colCx'); // c1c = c1c.length > 0 ? c1c : false;
			    c2c = ed.nextAll('.colDx'); // c2c = c2c.length > 0 ? c2c : false;
			    tem = $('#bloccoTemi').parent();	
			
			    // Apertura
			    // 3 colonne sopra Editor’s choice
			    // 2 colonne sopra Editor’s choice
			    // Editor’s choice
			    // 3 colonne sotto Editor’s Choice
			    // 2 colonne sotto Editor’s Choice
			    // 1 colonna sopra Editor’s Choice
			    // 1 colonna sotto Editor’s Choice
			    // box tematiche
			
			    c3a.insertAfter(ap);
			    c2a.insertAfter(c3a);
			    // ed.insertAfter(c2a);
			    c3c.insertAfter(ed);
			    c2c.insertAfter(c3c);
			    c1a.insertAfter(c2c);
			    c1c.insertAfter(c1a);
					
			    // if (ap.is('.big')) {
			    //   // sposto 2 colonne alto prima di editors choice
			    //   if (ed.length > 0) {
			    //     c2a.insertBefore(ed);
			    //   }
			    // }
			    // if (c2a.length > 0 && c1a.length > 0) {
			    //   // c1a.insertBefore(c2a);
			    // }
			    c3a.add(c3c).add(ap).add(ed).add(tem).add(".colDx").css('clear', 'both');
			    c2a.add(c2c).add(c1a).add(c1c).add(".colDx").each(function() {
			      var t, c;
			      t = $(this);
			      c = t.data('prev-class') || t.attr('class');
			      t.data('prev-class', c);
			      t.removeClass('colDx colCx').addClass('colSx');
			    });
			
			    // $('meta[name=viewport]').attr('content', 'width=475');
			
		  	}());
		}
	}
}
